    {
      "id": "largeNumber",
      "type": "number",
      "label": "Very large number",
      "required": true,
      "placeholder": "Enter a very large number",
      "validation": {
        "min": -999999999999999,
        "max": 999999999999999,
        "message": "Number must be between -999999999999999 and 999999999999999"
      }
    },
    {
      "id": "decimalNumber",
      "type": "number",
      "label": "Decimal number with many places",
      "required": true,
      "placeholder": "Enter a decimal number",
      "validation": {
        "min": 0.000001,
        "max": 999999.999999,
        "message": "Number must be between 0.000001 and 999999.999999"
      }
    },
    {
      "id": "longTextarea",
      "type": "textarea",
      "label": "Textarea with very long content",
      "required": true,
      "placeholder": "This is a very long placeholder text for a textarea field that should wrap properly and not cause any layout issues. It contains many words and should be displayed correctly.",
      "rows": 10,
      "validation": {
        "min": 1,
        "max": 10000,
        "message": "Text must be between 1 and 10000 characters"
      }
    },
    {
      "id": "complexConditional",
      "type": "text",
      "label": "Complex conditional field",
      "required": false,
      "placeholder": "This field has complex conditional logic",
      "conditional": {
        "field": "veryLongOptions",
        "operator": "equals",
        "value": "option1",
        "action": "show"
      }
    },
    {
      "id": "nestedConditional",
      "type": "text",
      "label": "Nested conditional field",
      "required": false,
      "placeholder": "This field depends on complex conditional",
      "conditional": {
        "field": "complexConditional",
        "operator": "not-empty",
        "action": "show"
      }
    },
    {
      "id": "checkboxWithLongLabel",
      "type": "checkbox",
